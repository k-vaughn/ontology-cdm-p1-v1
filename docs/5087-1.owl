    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasAggregateFunction">
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec21972/v1/Quantity"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Function"/>
    </owl:ObjectProperty>
    

