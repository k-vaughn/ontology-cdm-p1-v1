    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/AgentThing"/>
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- https://w3id.org/isoiec5087/-1/v1/agentInvolvedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/agentInvolvedIn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/involvesAgent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/commitsToActivity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/commitsToActivity">
        <skos:definition>A commitment to some activity</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/AtomicAgreement"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/establishedOn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/establishedOn">
