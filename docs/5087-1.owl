        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasDuty"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/AtomicAgreement"/>
