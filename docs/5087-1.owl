    <!-- https://w3id.org/isoiec5087/-1/v1/ActivityOntologyThing -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087/-1/v1/ActivityThing">
        <skos:definition>Added for organizational purposes, to identify all classes defined in the Activity pattern.</skos:definition>
