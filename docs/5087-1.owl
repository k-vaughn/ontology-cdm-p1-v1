        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasDecomp"/>
                <owl:qualifiedCardinality rdf:datatype="xsd:nonNegativeInteger">0</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


