    <!-- https://w3id.org/isoiec5087/-1/v1/performedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/performedBy">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/performs"/>
