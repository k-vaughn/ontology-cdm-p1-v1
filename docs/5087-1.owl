     -->
    <!-- https://w3id.org/isoiec5087/-1/v1/existsAt -->
    <!-- Issue: ISO/IEC 5087-1 does not restrict existsAt to be a FunctionalProperty. I would argue that at least in some contexts, the generic property existsAt could have multiple values (e.g., I might want to indicate multiple non-continuous time intervals) -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/existsAt">
        <skos:definition>Identifies the TemporalEntity that reflects the time Instant or Interval during which a Thing is valid.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <rdf:type rdf:resource="owl:FunctionalProperty"/>
        <rdfs:range rdf:resource="time:TemporalEntity"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasFirstManifestation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasFirstManifestation">
        <skos:definition>Identifies the First Manifestation that is related to a particular Manifestation (i.e., for some snapshot of an object, it identifies the first such snapshot of the object).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
        <rdf:type rdf:resource="owl:FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasLatestManifestation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasLatestManifestation">
        <skos:definition>Identifies the most recent Manifestation that is related to a particular FirstManifestation (i.e., for some snapshot of an object, it identifies the most recent snapshot of the object).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <rdf:type rdf:resource="owl:FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/FirstManifestation"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasManifestation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasManifestation">
        <skos:definition>identifies a Manifestation that captures the instantiation of variant attributes of an object at some time (i.e., a temporal snapshot of some individual). The individual is related to all manifestations in a series via the hasManifestation property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasNextManifestation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasNextManifestation">
        <skos:definition>Identifies the immediate successor Manifestation (i.e. the subsequent snapshot of the object).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <rdf:type rdf:resource="owl:FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasPreviousManifestation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasPreviousManifestation">
        <skos:definition>Identifies the immediate prior Manifestation (i.e. the prior snapshot of the object).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasNextManifestation"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/precedesManifestation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/precedesManifestation">
        <skos:definition>Identifies all subsequent Manifestations that follow a FirstManifestation.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="FoundationalCdmObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/FirstManifestation"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FirstManifestation"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


