                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087/-1/v1/AtomicAgreement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasPrivilege"/>
