<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/isoiec5087/-1/v1/"
     xml:base="https://w3id.org/isoiec5087/-1/v1/"
     xmlns:cc="http://creativecommons.org/ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:org="http://www.w3.org/ns/org#"
     xmlns:time="http://www.w3.org/2006/time#">
    <owl:Ontology rdf:about="https://w3id.org/isoiec5087/-1/v1/">
    	<!-- Identity -->
    	<dcterms:title>Information technology - City data model - Part 1: Foundation level concepts</dcterms:title>
        <rdfs:seeAlso rdf:resource="https://www.iso.org/standard/80718.html"/>
        <dcterms:bibliographicCitation>ISO/IEC 5087-1:2023</dcterms:bibliographicCitation>
    	<skos:definition>This ontology specifies the foundation-level concepts of the city data model.</skos:definition>
    	<vann:preferredNamespaceUri>https://w3id.org/isoiec5087/-1/v1/</vann:preferredNamespaceUri>
    	<vann:preferredNamespacePrefix>cdm1</vann:preferredNamespacePrefix>

    		
    	<!-- Provenance -->
        <dcterms:creator>Megan Katsumi</dcterms:creator>
        <dcterms:creator>Mark Fox</dcterms:creator>
    	<dcterms:creator>Kenneth Vaughn</dcterms:creator>
        <dcterms:issued rdf:datatype="xsd:date">2023</dcterms:issued> <!-- Date of the source document, comment out if draft -->
        <owl:priorVersion rdf:datatype="xsd:date">2017-11-05</owl:priorVersion> <!-- Date of prior ontology file, if any -->
		<dcterms:created rdf:datatype="xsd:date">2026-01-13</dcterms:created> <!-- Date of creation of this ontology file -->
        <owl:versionInfo>1.3.0</owl:versionInfo>
        <owl:versionIRI rdf:resource="https://w3id.org/isoiec5087/-1/v1/r3.0"/>
        <rdfs:comment>The prior version of this ontology was divided into a distinct file 
            for each pattern contained within Part 1; this version combines all of the 
            patterns into a single file for ease of reference. Some of the prior component 
            ontology files were labelled as v1.2. This update does not make any semantic
            changes from the approved ontology, but does correct several references 
            resulting in a change greater than a patch but less than a major change. As 
            such, it is published as v1.3.0.</rdfs:comment>
        
        <!-- License -->
        <cc:license>http://creativecommons.org/licenses/by/4.0/</cc:license>
        
        <!-- Imports -->
        <owl:imports rdf:resource="http://www.w3.org/ns/prov"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/org"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time"/>

    </owl:Ontology>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    <!-- https://w3id.org/isoiec5087/-1/v1/CdmObjectProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/CdmObjectProperty">
        <skos:definition>Added for organizational purposes, to identify all object properties defined in the City Data Model.</skos:definition>
    </owl:ObjectProperty>


    <!-- https://w3id.org/isoiec5087/-1/v1/FoundationalCdmObjectProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmObjectProperty">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/CdmObjectProperty"/>
        <skos:definition>Added for organizational purposes, to identify all foundational object properties defined in the City Data Model.</skos:definition>
    </owl:ObjectProperty>


    <!-- https://w3id.org/isoiec5087/-1/v1/achievedAt -->
    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/achievedAt">
        <skos:definition>Identifies the time Interval or Instant during which the State was satisfied.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdfs:range rdf:resource="time:TemporalEntity"/>
        <!-- Issue: Axioms not in ISO/IEC 5087-1 -->
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/effectOf"/>
            </rdf:Description>
            <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/endOf"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasDecomp"/>
            </rdf:Description>
            <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/conjunctiveAchievedAt"/>
            <rdf:Description rdf:about="http://www.w3.org/2006/time#intervalDuring"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/preconditionOf"/>
            </rdf:Description>
            <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/beginOf"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/beginOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/beginOf">
        <skos:definition>Identifies the time Instant at which the Activity occurs.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/FoundationalCdmObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="time:Instant"/>
        <!-- Issue: Axioms not in ISO/IEC 5087-1 -->
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/occursAt"/>
            <rdf:Description rdf:about="http://www.w3.org/2006/time#hasBeginning"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    

    <!-- Issue: ISO 5087-1 defines as causedByActivity -->
    <!-- https://w3id.org/isoiec5087/-1/v1/causedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/causedBy">
        <skos:definition>Identifies an Activity that in some (indirect) way caused the State to be realized. The causedBy property is the inverse of the causes property and is a generalization (super-property) of the effectOf property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/causes"/>
    </owl:ObjectProperty>
    


    <!-- Issue: ISO 5087-1 defines as causesState -->
    <!-- https://w3id.org/isoiec5087/-1/v1/causes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/causes">
        <skos:definition>identifies a State that in some (indirect) way was caused by the occurrence of the Activity. An Activity is caused by a State if the State is an effect of the Activity or if the State is an effect of some subactivity of the Activity. The causes property is a generalization (super-property) of the hasEffect property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
    </owl:ObjectProperty>
    

    <!-- Issue: Note defined in ISO/IEC 5087-1 and seems redundant (i.e., a subproperty to assign a new name without any additional sematics) -->
    <!-- https://w3id.org/isoiec5087/-1/v1/conjunctiveAchievedAt -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/conjunctiveAchievedAt">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/achievedAt"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/ConjunctiveState"/>
    </owl:ObjectProperty>
    


    <!-- Issue: Note defined in ISO/IEC 5087-1 and seems redundant (i.e., a subproperty to assign a new name without any additional sematics) -->
    <!-- https://w3id.org/isoiec5087/-1/v1/disjunctiveAchievedAt -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/disjunctiveAchievedAt">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/achievedAt"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/DisjunctiveState"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/effectOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/effectOf">
        <skos:definition>Identifies an Activity that the State was realized by.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasEffect"/>
    </owl:ObjectProperty>
    

    <!-- Issue: ISO 5087-1 defines as enabledByState -->
    <!-- https://w3id.org/isoiec5087/-1/v1/enabledBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/enabledBy">
        <skos:definition>Identifies a State that in some (indirect) way enabled the Activity to occur. An Activity is enabled by a State if the State is a precondition for the Activity or if the State is a precondition of some subactivity of the Activity. The enabledByState property is a generalization (super-property) of the hasPrecondition property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/enables"/>
    </owl:ObjectProperty>
    


    <!-- Issue: ISO 5087-1 defines as enablesActivity -->
    <!-- https://w3id.org/isoiec5087/-1/v1/enables -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/enables">
        <skos:definition>Identifies an Activity that in some (indirect) way was enabled by the State. The enables property is the inverse of the enabledBy property and is a generalization (super-property) of the preconditionOf property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/endOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/endOf">
        <skos:definition>Identifies the time Instant at which the Activity ends.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="time:Instant"/>
        <!-- Issue: Axioms not in ISO/IEC 5087-1 -->
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/isoiec5087/-1/v1/occursAt"/>
            <rdf:Description rdf:about="http://www.w3.org/2006/time#hasEnd"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasDecomp -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasDecomp">
        <skos:definition>Identifies two or more States into which the State may be immediately decomposed, i.e. its direct children.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasSubstate"/>
        <!-- Issue: Logical inconsistency; ISO 5087-1 defines the parent (hasSubState) with a domain of only NonTerminalState and a range of State. Do we then need to say that there there are exactly 0 States of hasDecomp for TerminalState? Do we need to show the following domain and range given that it is already inherited. The only customization of this class seems to be in the definition and the requirement for min2 (which by definition would also apply to hasSubState) -->
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasEffect -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasEffect">
        <skos:definition>Identifies a State that is realized once the Activity has occurred.</skos:definition>
        <!-- Issue: changed the subProperty to be of causes rather than causedBy to align with standard and other rules -->
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/causes"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasPrecondition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasPrecondition">
        <skos:definition>Identifies a State that must be realized in order for the Activity to occur.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/enabledBy"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/preconditionOf"/>
    </owl:ObjectProperty>
    

    <!-- Issue: hasStatus is used in two different contexts. To maximize reuse, I created subProperties which allow the parent to still work, but allows for precise definitions when applied. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/hasStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasStatus">
        <skos:definition>Identifies the status of the parent Thing at some point or interval in time.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
    </owl:ObjectProperty>
    
    <!-- https://w3id.org/isoiec5087/-1/v1/hasActivityStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasActivityStatus">
        <skos:definition>Identifies the status of the Activity at some point or interval in time. For example, the Activity may be “scheduled", “executing” or “completed”.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasStatus"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityStatus"/>
    </owl:ObjectProperty>
    
    <!-- https://w3id.org/isoiec5087/-1/v1/hasStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasStateStatus">
        <skos:definition>identifies the status of the State at some point or interval in time. For example, the State may be “unsatisfied", “satisfied” or “completed”.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/hasStatus"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/StateStatus"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087/-1/v1/hasSubactivity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasSubactivity">
        <skos:definition>Identifies a more granular Activity into which the Activity may be decomposed.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/hasSubstate -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/hasSubstate">
        <skos:definition>Identifies a State into which the complex state decomposes, at any level (i.e. its child state, grandchild state, etc.). The hasSubState property is transitive and a super-property of the hasDecomp property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/NonTerminalState"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdf:type rdf:resource="owl:TransitiveProperty"/>
    </owl:ObjectProperty>
    

    <!-- Issue: Not defined in ISO/IEC 5087-1; it only defines occursBefore -->
    <!-- https://w3id.org/isoiec5087/-1/v1/occursAfter -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/occursAfter">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/occursBefore"/>
    </owl:ObjectProperty>
    

    <!-- Issue: changed from time:TemporalEntity to time:Interval to align with ISO/IEC 5087-1 -->
    <!-- https://w3id.org/isoiec5087/-1/v1/occursAt -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/occursAt">
        <skos:definition>Identifies a time Interval over which the Activity occurs.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="time:Interval"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/occursBefore -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/occursBefore">
        <skos:definition>Identifies an Activity that the Activity occurred before.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdf:type rdf:resource="owl:TransitiveProperty"/>
        <rdfs:comment>An activity occursBefore another if its endOf instant is before the beginOf instant of the other activity; the occursBefore relation is transitive. We cannot define this semantics in OWL, though it could be captured in future work by an extension with rules.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- Issue: Not defined in ISO/IEC 5087-1; it only defines occursDirectlyBefore -->
    <!-- https://w3id.org/isoiec5087/-1/v1/occursDirectlyAfter -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/occursDirectlyAfter">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/occursAfter"/>
        <owl:inverseOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/occursDirectlyBefore"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/occursDirectlyBefore -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/occursDirectlyBefore">
        <skos:definition>Identifies an Activity that occurred immediately prior to the Activity. The
occursDirectlyBefore property is a sub-property of the occursBefore property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/occursBefore"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
        <rdfs:comment>An activity occursDirectlyBefore another if it occursAt an interval that meets the interval of the other activity. We cannot define this semantics in OWL, though it could be captured by an extension with rules.</rdfs:comment>
    </owl:ObjectProperty>
    

    <!-- Issue: Changed the subPropertyOf to align with ISO/IEC 5087-1 -->
    <!-- https://w3id.org/isoiec5087/-1/v1/preconditionOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/preconditionOf">
        <skos:definition>Identifies an Activity that requires the State to be realized in order to occur.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/enablesActivity"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/State"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/isoiec5087/-1/v1/satisfiedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/satisfiedBy">
        <skos:definition>Specifies the Manifestation that is to be satisfied (i.e., realized).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/ManifestationState"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087/-1/v1/Manifestation"/>
    </owl:ObjectProperty>
    


    <!-- Issue: scheduledFor is used in two different contexts. To maximize reuse, I created subProperties which allow the parent to still work, but allows for precise definitions when applied. -->
    <!-- https://w3id.org/isoiec5087/-1/v1/scheduledFor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/scheduledFor">
        <skos:definition>Identifies a time Interval during which a Thing occurs.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/ActivityOntologyProperty"/>
        <rdfs:range rdf:resource="time:Interval"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087/-1/v1/activityScheduledFor">
        <skos:definition>Identifies the time Interval that an Activity was scheduled to be performed/occur at.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087/-1/v1/scheduledFor"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087/-1/v1/Activity"/>
    </owl:ObjectProperty>
    
