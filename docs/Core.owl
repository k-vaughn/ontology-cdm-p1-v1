    <!-- https://w3id.org/citydata/part1/v1/CdmThing -->

    <owl:Class rdf:about="https://w3id.org/citydata/part1/v1/CdmThing">
        <skos:definition>Added for organizational purposes, to identify all classes defined in the City Data Model.</skos:definition>
    </owl:Class>


    <!-- https://w3id.org/citydata/part1/v1/FoundationalCdmThing -->

    <owl:Class rdf:about="https://w3id.org/citydata/part1/v1/FoundationalCdmThing">
        <skos:definition>Added for organizational purposes, to identify all classes defined in the Foundation-level CDM ontology.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part1/v1/CdmThing"/>
    </owl:Class>


</rdf:RDF>    